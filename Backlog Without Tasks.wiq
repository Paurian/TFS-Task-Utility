<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://cstfs:8080/tfs/engineering</TeamFoundationServer><TeamProject>Clarity</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [System.Tags] FROM WorkItemLinks WHERE ([Source].[System.WorkItemType] IN ('Bug', 'Product Backlog Item')  AND  [Source].[System.AreaPath] UNDER 'Clarity\Sustaining'  AND  [Source].[System.IterationId] IN (2031, 2388)  AND  [Source].[System.State] &lt;&gt; 'Removed'  AND  [Source].[System.State] &lt;&gt; 'Resolved') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Task') ORDER BY [System.Id] mode(DoesNotContain)</Wiql></WorkItemQuery>